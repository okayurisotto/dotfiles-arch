# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D

# unmap {{{

keymap	C-@	NULL	# MARK
keymap	C-_	NULL	# GOTO_HOME
keymap	C-a	NULL	# LINE_BEGIN
keymap	C-b	NULL	# MOVE_LEFT
keymap	C-e	NULL	# LINE_END
keymap	C-f	NULL	# MOVE_RIGHT
keymap	C-g	NULL	# LINE_INFO
