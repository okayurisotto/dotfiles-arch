# map {{{

# In-page Navigation {{{

keymap	h	MOVE_LEFT1
keymap	j	MOVE_DOWN1
keymap	k	MOVE_UP1
keymap	l	MOVE_RIGHT1

keymap	0	LINE_BEGIN
keymap	\^	LINE_BEGIN
keymap	SPCh	LINE_BEGIN

keymap	$	LINE_END
keymap	SPCl	LINE_END

keymap	w	NEXT_WORD
keymap	b	PREV_WORD

keymap	TAB	NEXT_LINK
keymap	M-[Z	PREV_LINK

keymap	J	UP
keymap	K	DOWN

keymap	SPCSPC	CENTER_V

keymap	gg	BEGIN
keymap	SPCk	BEGIN

keymap	G	END
keymap	SPCj	END

# }}}

# Hyperlink Operations {{{

keymap	C-j	GOTO_LINK
keymap	I	VIEW_IMAGE
keymap	=	INFO
keymap	:	MARK_URL
keymap	o	EXTERN

# }}}

# File/Stream Operations {{{

keymap	C-l	GOTO

# }}}

# Buffer Operations {{{

keymap	B	BACK
keymap	H	PREV
keymap	L	NEXT
keymap	r	RELOAD

# }}}

# Tab Operations {{{

keymap	t	NEW_TAB
keymap	x	CLOSE_TAB
keymap	]	NEXT_TAB
keymap	[	PREV_TAB
keymap	.	TAB_RIGHT
keymap	,	TAB_LEFT

# }}}

# Bookmark Management {{{

keymap	C-b	VIEW_BOOKMARK
keymap	C-d	ADD_BOOKMARK

# }}}

# Searches {{{

keymap	/	WHEREIS
keymap	?	SEARCH_BACK
keymap	n	SEARCH_NEXT
keymap	N	SEARCH_PREV

# }}}

# misc {{{

keymap	C-h	HELP
keymap	C-o	OPTIONS
keymap	C-y	HISTORY
keymap	!	SHELL
keymap	q	EXIT

# }}}

# }}}

# shell {{{

keymap	ep	SHELL	'mpv --loop=inf $W3M_URL'
keymap	ey	SHELL	'echo $W3M_URL | clip -i'

# }}}
